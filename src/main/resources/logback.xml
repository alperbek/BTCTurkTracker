<configuration scan="true">
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-23date{yyyy-MM-dd HH:mm:ss.S} %-5level [%-25logger{15}] %msg%n</pattern>
        </encoder>
    </appender>
    <root level="WARN">
        <appender-ref ref="CONSOLE" />
    </root>
    <logger name="com.github.vy.btcturktracker" level="INFO"/>
</configuration>
